@model List<FabPonto.Models.User>

<div>
    <table>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th></th>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Name</td>
                <td>@user.Email</td>
                <td></td>
            </tr>
        }
    </table>
    <a href="@Url.Action("Index","Home")" class="btn btn-default">Registrar</a>
</div>